<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.express_eat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_express_eat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_express_eat_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".Dominos_Food"></activity>
        <activity android:name=".Wendys_Food" />
        <activity android:name=".Popeyes_Food" />
        <activity android:name=".BurgerKing_Food" />
        <activity android:name=".Jollibee_Food" />
        <activity android:name=".PopeyesMenu" />
        <activity android:name=".BurgerKingMenu" />
        <activity android:name=".WendysMenu" />
        <activity android:name=".DominosMenu" />
        <activity android:name=".JollibeeMenu" />
        <activity
            android:name=".FoodDisplay"
            android:label="Details" />
        <activity android:name=".McdonaldsMenu" />
        <activity android:name=".Mcdo_Food" />
        <activity
            android:name=".Settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Offers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Restaurant"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ForgotPassword"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LogIn"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".start_page"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Get_started"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyCart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OrderDone"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Summary"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MyOrderEmpty"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Tracker"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>